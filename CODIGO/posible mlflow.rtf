\cf8 \cb7 # Dockerfile para empaquetar el modelo en un contenedor\
FROM python:3.8\
\
# Instala MLflow y otras dependencias\
RUN pip install mlflow\
\
# Copia el modelo registrado en la imagen del contenedor\
COPY ./mlruns /mlruns\
\
# Establece el punto de entrada para servir el modelo (puede variar seg\'fan tu modelo)\
CMD ["mlflow", "models", "serve", "-m", "runs:/<RUN_ID>/modelo", "--host", "0.0.0.0", "--port", "5000"]\
